# Telegram API credentials
TELEGRAM_API_ID=your_api_id_here
TELEGRAM_API_HASH=your_api_hash_here
TELEGRAM_PHONE_NUMBER=your_phone_number_here

# OpenAI API key
OPENAI_API_KEY=your_openai_api_key_here

# Optional: Proxy settings
# PROXY_TYPE=socks5          # socks5, socks4, or http
# PROXY_ADDR=1.1.1.1        # IP address or hostname
# PROXY_PORT=5555           # Port number
# PROXY_USERNAME=username   # Optional: proxy authentication
# PROXY_PASSWORD=password   # Optional: proxy authentication
# PROXY_RDNS=true          # Optional: remote DNS resolution (default: true)
